{
  "name": "angular-templatecache-extract",
  "version": "1.1.0",
  "description": "Extract angular's templates from templatecache",
  "main": "lib/index.js",
  "dependencies": {
    "bluebird": "^3.5.3",
    "espree": "^5.0.0",
    "esprima": "^4.0.1",
    "esquery": "^1.0.1",
    "static-eval": "^2.0.0"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^2.0.0",
    "@ava/babel-preset-transform-test-files": "^4.0.0",
    "@babel/cli": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "eslint": "^5.12.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  },
  "scripts": {
    "prepare": "babel src -d lib",
    "test": "eslint src/*js tests/*.js && ava"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "globals": {
      "angular": true
    }
  },
  "ava": {
    "files": [
      "tests/**/*.tests.js"
    ],
    "require": [
      "@babel/register"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false,
        "presets": [
          "@ava/transform-test-files",
          "@ava/stage-4"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/komachi/angular-templatecache-extract.git"
  },
  "keywords": [
    "angular",
    "template",
    "dom",
    "html"
  ],
  "author": "Anton Nesterov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/komachi/angular-templatecache-extract/issues"
  },
  "homepage": "https://github.com/komachi/angular-templatecache-extract#readme"
}
